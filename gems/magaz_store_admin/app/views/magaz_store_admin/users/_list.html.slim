= paginate @users

table class="table table-striped"
  thead
    tr
      th = t('.user_email')
      th

  tbody
    - @users.each do |user|
      tr
        td = link_to user.email, [ user]
        td = link_to t('.delete'), [ user], method: :delete, data: { confirm: t('.confirmation') }

br

= paginate @users